{
    "name": "pitangent/workflow",
    "description": "I've created this package to maintaining our code stability on Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Khadimul Miah",
            "email": "khadimul@pitangent.com",
            "homepage": "http://pitangent.com"
        }
    ],
    "homepage": "https://github.com/ows/workflow",
    "keywords": ["Laravel", "Workflow"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "illuminate/auth": "~5|~6|~7|~8",
        "illuminate/container": "~5|~6|~7|~8",
        "illuminate/contracts": "~5|~6|~7|~8",
        "illuminate/database": "~5|~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Pitangent\\Workflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pitangent\\Workflow\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pitangent\\Workflow\\WorkflowServiceProvider",
                "Pitangent\\Workflow\\PermissionServiceProvider"
            ],
            "aliases": {
                "Workflow": "Pitangent\\Workflow\\Facades\\Workflow"
            }
        }
    }
}
